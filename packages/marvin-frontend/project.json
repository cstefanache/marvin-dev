{
  "name": "marvin-frontend-new",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/marvin-frontend-new/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/packages/marvin-frontend-new",
        "index": "packages/marvin-frontend-new/src/index.html",
        "baseHref": "/",
        "main": "packages/marvin-frontend-new/src/main.tsx",
        "polyfills": "packages/marvin-frontend-new/src/polyfills.ts",
        "tsConfig": "packages/marvin-frontend-new/tsconfig.app.json",
        "assets": [
          "packages/marvin-frontend-new/src/favicon.ico",
          "packages/marvin-frontend-new/src/assets"
        ],
        "styles": ["packages/marvin-frontend-new/src/styles.scss"],
        "scripts": [],
        "webpackConfig": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "packages/marvin-frontend-new/src/environments/environment.ts",
              "with": "packages/marvin-frontend-new/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "marvin-frontend-new:build",
        "hmr": true,
        "port": 9080
      },
      "configurations": {
        "development": {
          "buildTarget": "marvin-frontend-new:build:development",
          "port": 9080
        },
        "production": {
          "buildTarget": "marvin-frontend-new:build:production",
          "hmr": false,
          "port": 9080
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/marvin-frontend-new/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/marvin-frontend-new/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
