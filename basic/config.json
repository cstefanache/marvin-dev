{
    "path": "basic",
    "rootUrl": "",
    "defaultTimeout": 10000,
    "urlReplacers": [
        {
            "regex": "({){0,1}[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}(}){0,1}",
            "alias": "{resourceId}"
        },
        {
            "regex": "(?<=recommended/)[A-Za-z0-9-]+",
            "alias": "{resourceName}"
        }
    ],
    "discoveryOptimizer": [
        {
            "tag": "id",
            "value_pattern": "(mui-.*)[^\"][^\"]"
        }
    ],
    "aliases": {
        "info": [
            {
                "name": "alert",
                "selectors": [".MuiAlert-message"]
            }
        ],
        "action": [
            {
                "name": "Select Company",
                "skipOptimizer": true,
                "selectors": ["form > div"]
            },
            {
                "name": "Navigation",
                "selectors": ["nav a"]
            },
            {
                "name": "Service Card Title",
                "selectors": ["h6.MuiTypography-root"]
            }
        ],
        "iterators": [
            {
                "name": "Select Company By Name",
                "selectors": ["form > div"],
                "identifiers": [
                    {
                        "name": "Company Name"
                    }
                ]
            },
            {
                "name": "Service Card",
                "selectors": ["section > div"],
                "identifiers": [
                    {"name": "title", "selector": "h6.MuiTypography-root"}
                ]
            },
            {
                "name": "Service Table Row",
                "selectors": ["tr"],
                "identifiers": [
                    {"name": "first column", "selector": "td:nth-child(1)"}
                ]
            }
        ]
    }
}
